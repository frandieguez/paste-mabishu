<h1>New paste</h1>

<% form_for(@paste) do |f| %>
  <%= f.error_messages %>
  <%= collection_select("paste", "language_id", @languages, "id", "name") %>
  <p>
    <%= f.label :content %><br />
    <%= f.text_area :content %>
  </p>
  <p>
    <%= f.label :language_id %><br />
    <%= f.text_field :language_id %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', pastes_path %>
